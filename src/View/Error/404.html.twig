{% extends 'layout.html.twig' %}
{% block title %}ERREUR 404
{% endblock %}
{% block stylesheets %}
	{{ parent() }}
{% endblock %}
{% block content %}
	<div class="container">
		<div class="row">
			<div class="col-md-12">
				<div class="error-template">
					<h1>
						Oups!</h1>
					<h2>
						Erreur 404</h2>
					<div class="error-details">
						Désolé, une erreur s'est produite, la page demandée n'a pas été trouvée!
					</div>
					<div class="error-actions">
						<a href="/product" class="btn btn-primary btn-lg">
							<span class="glyphicon glyphicon-home"></span>
							Retour à l'accueil
						</a>
					</div>
				</div>
			</div>
		</div>
	</div>
{% endblock %}
